<div class="container">
  <h1>New Product</h1>

  <%= form_for @product do |f|  %>

  <%= f.label :name %>
  <%=  f.text_field :name %>
  <br>
  <%= f.label :description %>
  <%=  f.text_field :description %>
  <br>
  <%= f.label :country_of_origin %>
  <%=  f.text_field :country_of_origin %>
  <br>
  <%= f.label :price %>
  <%=  f.text_field :price %>
  <br>
  <%= f.submit %>
  <% end %>

</div>
