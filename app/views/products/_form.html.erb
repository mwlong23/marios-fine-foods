<div class="container">
  <h1>New Product</h1>

  <%= form_form @list do |f|  %>

  <%= f.label :name %>
  <%=  f.text_field :name %>

  <%= f.label :description %>
  <%=  f.text_field :description %>

  <%= f.label :country_of_origin %>
  <%=  f.text_field :country_of_origin %>

  <%= f.label :price %>
  <%=  f.text_field :price %>

  <%= f.submit %>
  <% end %>

</div>
